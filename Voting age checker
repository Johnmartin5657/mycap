#include<iostream>
using namespace std;
int main()
{
    int age;
    cout<<"Enter age of the candidate:- \n";
    cin>>age;

    if(age>=18)
    {
        cout<<"Candidate is eligible to Vote.\n ";
    }
    else
    cout<<"Candidate is not eligible to Vote.\n";
    cout<<18-age<<"years left till you can't vote\n";
    return 0;
}
